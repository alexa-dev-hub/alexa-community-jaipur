{
  "name": "alexa-community-jaipur",
  "version": "1.0.0",
  "description": "Server for Alexa Community Jaipur",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server_dev": "nodemon app.js",
    "server": "node app.js",
    "client": "npm start --prefix ./client",
    "dev": "concurrently \"npm run server_dev\" \"npm run client\"",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "action-ci": "npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Tarun Singh",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.17.4",
    "nodemon": "^2.0.4"
  }
}
